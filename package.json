{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "new-llms",
  "title": "New LLMs",
  "description": "Quickly check if any new affordable LLMs have been added to the list",
  "icon": "command-icon.png",
  "author": "itsBREX",
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "New LLMs",
      "subtitle": "Keeping track of affordable LLMs",
      "description": "Checks a static markdown file hosted in a github repo for changes",
      "mode": "view"
    }
  ],
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "^18.2.0",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "@types/node-fetch": "^2.6.3",
    "@types/react-dom": "^18.2.1",
    "node-fetch": "^3.3.1"
  }
}
